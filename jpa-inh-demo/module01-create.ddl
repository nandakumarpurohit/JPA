CREATE TABLE ITEM (ID BIGINT NOT NULL, DTYPE VARCHAR(31), DESCRIPTION VARCHAR(3000), TITLE VARCHAR(100), unit_cost FLOAT, GENRE VARCHAR(255), total_duration FLOAT, ISBN VARCHAR(15), nb_of_pages INTEGER, publication_date DATE, PRIMARY KEY (ID))
CREATE TABLE MUSICIAN (ID BIGINT NOT NULL, BIO VARCHAR(5000), date_of_birth DATE, first_name VARCHAR(50), last_name VARCHAR(50), preferred_instrument VARCHAR(255), PRIMARY KEY (ID))
CREATE TABLE ITEM_MUSICIAN (CD_ID BIGINT NOT NULL, musicians_ID BIGINT NOT NULL, PRIMARY KEY (CD_ID, musicians_ID))
ALTER TABLE ITEM_MUSICIAN ADD CONSTRAINT FK_ITEM_MUSICIAN_musicians_ID FOREIGN KEY (musicians_ID) REFERENCES MUSICIAN (ID)
ALTER TABLE ITEM_MUSICIAN ADD CONSTRAINT FK_ITEM_MUSICIAN_CD_ID FOREIGN KEY (CD_ID) REFERENCES ITEM (ID)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
